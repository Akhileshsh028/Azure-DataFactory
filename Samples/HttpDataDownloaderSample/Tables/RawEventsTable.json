{  
   "name":"RawEventsTable",
   "properties":{  
      "location":{  
         "type":"AzureBlobLocation",
         "folderPath":"<Container>/<folder>/yearno={Year}/monthno={Month}/dayno={Day}/",
         "partitionedBy":[  
            {  
               "name":"Year",
               "value":{  
                  "type":"DateTime",
                  "date":"SliceStart",
                  "format":"yyyy"
               }
            },
            {  
               "name":"Month",
               "value":{  
                  "type":"DateTime",
                  "date":"SliceStart",
                  "format":"%M"
               }
            },
            {  
               "name":"Day",
               "value":{  
                  "type":"DateTime",
                  "date":"SliceStart",
                  "format":"%d"
               }
            }
         ],
         "linkedServiceName":"RawEventsLinkedService"
      },
      "availability":{  
         "frequency":"Hour",
         "interval":1
      }
   }
}