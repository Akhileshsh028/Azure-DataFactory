{  
   "name":"ContosoTweetsAggSentimentBlob",
   "properties":{  
      "structure":[  
         {  
            "name":"TweetSentimentCount",
            "position":0,
            "type":"Int"
         },
         {  
            "name":"TweetSentiment",
            "position":1,
            "type":"String"
         }
      ],
      "location":{  
         "type":"AzureBlobLocation",
         "folderPath":"twitteranalysis/twitter/aggregate/Sentiments/",         
         "format":{  
            "type":"TextFormat",
