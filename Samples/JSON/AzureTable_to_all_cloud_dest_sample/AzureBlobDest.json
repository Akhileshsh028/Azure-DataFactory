{
    "name": "AzureBlobDest",
    "properties":
    {
    "location":
      {
        "type": "AzureBlobLocation",
        "blobPath": "$$Text.Format('Container/folder/{0:yyyyMMddHHmm}', SliceStart)",
        "linkedServiceName": "StorageLinkedService",
        "format":
        {
            "type": "TextFormat",
            "columnDelimiter": "\t",
            "rowDelimiter": "\n"
        }
      },
    "availability":
      {
        "frequency": "Hour",
        "interval": 1
      }
    }
}
