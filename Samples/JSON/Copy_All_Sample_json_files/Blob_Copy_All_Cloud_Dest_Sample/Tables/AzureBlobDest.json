{  
   "name":"AzureBlobDest",
   "properties":{  
      "location":{  
         "type":"AzureBlobLocation",
         "folderPath":"container/dest/{PartitionKey}",
         "linkedServiceName":"<StorageLinkedService>",
         "format":{  
            "type":"TextFormat",
            "columnDelimiter":"\t",
            "rowDelimiter":"\n"
         },
         "partitionedBy":[  
            {  
               "name":"PartitionKey",
               "value":{  
                  "type":"DateTime",
                  "date":"SliceStart",
                  "format":"yyyyMMdd-HH"
               }
            }
         ]
      },
      "availability":{  
         "frequency":"Hour",
         "interval":1
      }
   }
}