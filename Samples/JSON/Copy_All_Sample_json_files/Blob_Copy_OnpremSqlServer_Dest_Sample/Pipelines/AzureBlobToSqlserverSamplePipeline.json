{  
   "name":"AzureBlobToSqlserverSamplePipeline",
   "properties":{  
      "description":"AzureBlobToSqlServerSamplePipeline",
      "activities":[  
         {  
            "name":"AzureBlobtoSQL",
            "description":"AzureBlobtoSQL",
            "type":"CopyActivity",
            "inputs":[  
               {  
                  "name":"AzureBlobSource"
               }
            ],
            "outputs":[  
               {  
                  "name":"sqlserverdest"
               }
            ],
            "transformation":{  
               "source":{  
                  "type":"BlobSource"
               },
               "sink":{  
                  "type":"SqlSink"
               }
            },
            "policy":{  
               "concurrency":1,
               "executionPriorityOrder":"OldestFirst",
               "retry":0,
               "timeout":"01:00:00"
            }
         }
      ]
   }
}